PROC SEC_JOB_OCC_LEVEL_1

preproc

	savepartial();

	ask if MJJ_MULT=2;

	// if entering a new occupation and the interviewer previously viewed all occupations, start on that screen
	if visualvalue(SEC_JOB_OCC_LEVEL_1) = notappl and loadsetting(ViewAllOccupationsSettingKey) = "1" then
		SEC_JOB_OCC_LEVEL_1 = 10;
		noinput;
	endif;

postproc

	if SEC_JOB_OCC_LEVEL_1 = 10 then // view all occupations
		savesetting(ViewAllOccupationsSettingKey,"1");
		skip to SEC_JOB_OCC_LEVEL_4;

	else
		savesetting(ViewAllOccupationsSettingKey,"0");

		if SEC_JOB_OCC_LEVEL_1 = 0 then // armed services occupations don't have sub major or minor groups
			SEC_JOB_OCC_LEVEL_2 = 0;
			SEC_JOB_OCC_LEVEL_3 = 0;
			advance to SEC_JOB_OCC_LEVEL_4;
		endif;

	endif;


PROC SEC_JOB_OCC_LEVEL_2

{
preproc

	ask if SEC_JOB_OCC_LEVEL_1 <> notappl and MJJ_MULT=2;

onfocus

	StartVS();

	numeric firstOccupation = SEC_JOB_OCC_LEVEL_1 * 10;
	numeric lastOccupation = firstOccupation + 9;

	do numeric ctr = firstOccupation while ctr <= lastOccupation

		if invalueset(ctr,SEC_JOB_OCC_LEVEL_2_VS1) then
			AddNumericVS(ctr,getlabel(SEC_JOB_OCC_LEVEL_1_VS1,ctr));
		endif;

	enddo;

	EndVS();

	setvalueset(SEC_JOB_OCC_LEVEL_2,vsCodes,vsLabels);

	// this field does not need to be filled for armed services occupations
	if SEC_JOB_OCC_LEVEL_1 = 0 then
		setproperty(SEC_JOB_OCC_LEVEL_2,"Protected","Yes");
	else
		setproperty(SEC_JOB_OCC_LEVEL_2,"Protected","No");
	endif;

}



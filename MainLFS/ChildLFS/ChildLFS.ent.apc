PROC MAIN_JOB_OCC_LEVEL_C1

preproc

	savepartial();

	//ask if MJJ_EMP_REL in 1,4,5;
	//ask if GLO_EMP = 1 ;

	// if entering a new occupation and the interviewer previously viewed all occupations, start on that screen
	if visualvalue(MAIN_JOB_OCC_LEVEL_C1) = notappl and loadsetting(ViewAllOccupationsSettingKey) = "1" then
		MAIN_JOB_OCC_LEVEL_C1 = 10;
		noinput;
	endif;

postproc

	if MAIN_JOB_OCC_LEVEL_1 = 10 then // view all occupations
		savesetting(ViewAllOccupationsSettingKey,"1");
		skip to MAIN_JOB_OCC_LEVEL_C4;

	else
		savesetting(ViewAllOccupationsSettingKey,"0");

		if MAIN_JOB_OCC_LEVEL_C1 = 0 then // armed services occupations don't have sub major or minor groups
			MAIN_JOB_OCC_LEVEL_C2 = 0;
			MAIN_JOB_OCC_LEVEL_C3 = 0;
			advance to MAIN_JOB_OCC_LEVEL_C4;
		endif;

	endif;
PROC MAIN_JOB_OCC_LEVEL_C2


preproc

	ask if MAIN_JOB_OCC_LEVEL_C1 <> notappl ;

onfocus

	StartVS();

	numeric firstOccupation = MAIN_JOB_OCC_LEVEL_C1 * 10;
	numeric lastOccupation = firstOccupation + 9;

	do numeric ctr = firstOccupation while ctr <= lastOccupation

		if invalueset(ctr,MAIN_JOB_OCC_LEVEL_C2_VS1) then
			AddNumericVS(ctr,getlabel(MAIN_JOB_OCC_LEVEL_C2_VS1,ctr));
		endif;

	enddo;

	EndVS();

	setvalueset(MAIN_JOB_OCC_LEVEL_C2,vsCodes,vsLabels);

	// this field does not need to be filled for armed services occupations
	if MAIN_JOB_OCC_LEVEL_C1 = 0 then
		setproperty(MAIN_JOB_OCC_LEVEL_C2,"Protected","Yes");
	else
		setproperty(MAIN_JOB_OCC_LEVEL_C2,"Protected","No");
	endif;


PROC MAIN_JOB_OCC_LEVEL_C3


preproc

	ask if MAIN_JOB_OCC_LEVEL_C1 <> notappl {and GLO_EMP = 1};

onfocus

	StartVS();

	numeric firstOccupation = MAIN_JOB_OCC_LEVEL_C2 * 10;
	numeric lastOccupation = firstOccupation + 9;

	do numeric ctr = firstOccupation while ctr <= lastOccupation

		if invalueset(ctr,MAIN_JOB_OCC_LEVEL_C3_VS1) then
			AddNumericVS(ctr,getlabel(MAIN_JOB_OCC_LEVEL_C3_VS1,ctr));
		endif;

	enddo;

	EndVS();

	setvalueset(MAIN_JOB_OCC_LEVEL_C3,vsCodes,vsLabels);

	// this field does not need to be filled for armed services occupations
	if MAIN_JOB_OCC_LEVEL_C1 = 0 then
		setproperty(MAIN_JOB_OCC_LEVEL_C3,"Protected","Yes");
	else
		setproperty(MAIN_JOB_OCC_LEVEL_C3,"Protected","No");
	endif;
PROC MAIN_JOB_OCC_LEVEL_C4


preproc

	ask if MAIN_JOB_OCC_LEVEL_C1 <> notappl {and GLO_EMP = 1};

onfocus

	if MAIN_JOB_OCC_LEVEL_C1 = 10 then // view all occupations
		setvalueset(MAIN_JOB_OCC_LEVEL_C4,MAIN_JOB_OCC_LEVEL_C4_VS1);

	else
		StartVS();

		numeric firstOccupation = MAIN_JOB_OCC_LEVEL_C3 * 10;
		numeric lastOccupation = firstOccupation + 9;

		do numeric ctr = firstOccupation while ctr <= lastOccupation

			if invalueset(ctr,MAIN_JOB_OCC_LEVEL_C4_VS1) then
				AddNumericVS(ctr,getlabel(MAIN_JOB_OCC_LEVEL_C4_VS1,ctr));
			endif;

		enddo;

		EndVS();

		setvalueset(MAIN_JOB_OCC_LEVEL_C4,vsCodes,vsLabels);

	endif;

postproc

	if MAIN_JOB_OCC_LEVEL_C4 = -999 then
		reenter MAIN_JOB_OCC_LEVEL_C1;
	endif;



PROC MAIN_JOB_OCC_LEVEL_C4


preproc

	ask if MAIN_JOB_OCC_LEVEL_C1 <> notappl {and GLO_EMP = 1};

onfocus

	if MAIN_JOB_OCC_LEVEL_C1 = 10 then // view all occupations
		setvalueset(MAIN_JOB_OCC_LEVEL_C4,MAIN_JOB_OCC_LEVEL_C4_VS1);

	else
		StartVS();

		numeric firstOccupation = MAIN_JOB_OCC_LEVEL_C3 * 10;
		numeric lastOccupation = firstOccupation + 9;

		do numeric ctr = firstOccupation while ctr <= lastOccupation

			if invalueset(ctr,MAIN_JOB_OCC_LEVEL_C4_VS1) then
				AddNumericVS(ctr,getlabel(MAIN_JOB_OCC_LEVEL_C4_VS1,ctr));
			endif;

		enddo;

		EndVS();

		setvalueset(MAIN_JOB_OCC_LEVEL_C4,vsCodes,vsLabels);

	endif;

